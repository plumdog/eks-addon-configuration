{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "auditLogDetections": {
      "additionalProperties": false,
      "description": "Audit log detection parameters",
      "properties": {
        "eks": {
          "properties": {
            "disable_audit": {
              "type": "boolean"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "deployment": {
      "additionalProperties": false,
      "description": "Uptycs Collector deployment",
      "properties": {
        "spec": {
          "properties": {
            "containers": {
              "properties": {
                "resources": {
                  "description": "Resources allocated to Uptycs Collector deployment",
                  "type": "object"
                }
              },
              "type": "object"
            },
            "hostname": {
              "description": "Cluster name that shows up in Uptycs UI",
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "uptycsCloud": {
      "description": "Uptycs cloud service onto which the cluster is enrolled",
      "type": "string"
    }
  },
  "required": [
    "deployment",
    "uptycsCloud",
    "auditLogDetections"
  ],
  "title": "Values",
  "type": "object"
}
